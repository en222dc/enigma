<Window x:Class="Enigma.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Enigma"
        mc:Ignorable="d"
        
        Title="MainWindow" Height="450" Width="900" Background="#463A5F">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        
        <Frame Grid.Row="0" Name="MenuFrame"/>
        <Frame Grid.Row ="1" Name="MainFrame"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="243,208,0,0" Grid.Row="1" VerticalAlignment="Top" Click="Button_Click"/>



    </Grid>
</Window>
